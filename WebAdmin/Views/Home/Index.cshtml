                                    <div class="d-flex mb-2">
                                        <div>
                                            <p class="mb-0 font-weight-bold">ABC</p>
                                            <h2 class="mb-0">2,346</h2>
                                        </div>
                                        <div class="ml-auto align-self-end">
                                            <p class="mb-0 font-14 text-danger">
                                                <i class='bx bxs-down-arrow-circle'></i>  <span>130.68% 31 days ago</span>
                                            </p>
                                        </div>
                                    </div>
